#define SendEvent(event) [[[GAI sharedInstance] defaultTracker] send:[[GAIDictionaryBuilder createEventWithCategory:@"event" action:event label:@"event" value:nil] build]];

#define SendError(class, error) [[[GAI sharedInstance] defaultTracker] send:[[GAIDictionaryBuilder createEventWithCategory:@"application_error" action:NSStringFromClass(class) label:error value:nil] build]];

#define SendPage(page) \
id tracker = [[GAI sharedInstance] defaultTracker]; \
[tracker set:kGAIScreenName value:page]; \
[tracker send:[[GAIDictionaryBuilder createAppView] build]];